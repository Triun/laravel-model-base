{
    "name": "triun/laravel-model-base",
    "description": "Generate Eloquent Model Bases for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "model", "eloquent", "base", "make", "generator"],
    "authors": [
        {
            "name": "Gonzalo Moreno",
            "email": "info@gonzalom.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.0,<5.4",
        "illuminate/console": "^5.0,<5.4",
        "illuminate/filesystem": "^5.0,<5.4",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "illuminate/database": "^5.2",
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
        "doctrine/dbal": "Load information from the database about models for phpdocs (~2.3)"
    },
    "autoload": {
        "psr-4": {
            "Triun\\ModelBase\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/TestCase.php"]
    }
}
